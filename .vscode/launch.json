{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug server",
      "cwd": "${workspaceFolder}",
      "sourceMaps": true,
      "preLaunchTask": "npm: build",
      "program": "${workspaceFolder}/dist/index",
      "stopOnEntry": false,
      "localRoot": "${workspaceFolder}",
      "outFiles": ["${workspaceFolder}/dist/**"],
      "skipFiles": ["${workspaceFolder}/node_modules/**/*.js"]
    }
  ]
}
